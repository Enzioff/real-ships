                <div class="t-header__col">
                    <div class="t-header__burger" :class="{_open: openMenu}" @click="openCloseMenu">
                        {% include "@components/ui-components/burger/template.twig" %}
                    </div>
                </div>
                <div class="t-header__col">
                    <div class="t-header__locale">
                        <locale-cmp></locale-cmp>
                    </div>
                </div>
            </div>
            <div v-if="openMenu" class="t-header__block-bottom">
                <div class="t-header__info">
                    <div class="t-header__menu">
                        {# переменные для компонента menu
                            - menuList: массив с данными
                            - type: свойство для стилей ('horizontal', 'vertical')
                        #}
                        {% include "@components/ui-components/menu/template.twig" with {
                            menuList: menu,
                            type: 'horizontal'
                        } %}
                    </div>
                    <div class="t-header__contacts">
                        <div class="t-header__contacts-phones">
                            {% for item in phones %}
                                <div class="t-header__contacts-phones-item">
                                    <span class="t-header__contacts-name">{{item.department}}</span>
                                    {% for phone in item.phones %}
                                        <a href="tel:{{ phone|raw }}" class="t-header__contacts-value">{{phone}}</a> 
                                    {% endfor %}
                                    
                                </div>
                            {% endfor %}
                        </div>
                        <div class="t-header__contacts-mail">
                            <span class="t-header__contacts-name">{{ t('app.front', 'Почта') }}</span>
                            <a href="mailto:{{ email|raw }}" class="t-header__contacts-value">{{email}}</a>
                        </div>
                    </div>
                </div>
                <div class="t-header__boats">
                    <swiper class="t-header__boats-slider" :options="swiperHeaderBoats">
                        {% for item in privateBoats %}
                            <swiper-slide class="t-header__boats-slider-item">
                                <a v-link href="{{item.link}}"  @click="closeMenu"  class="t-boats__item">
                                    <div class="t-boats__item-img">
                                        {% if item.icon %}
                                            <img src="{{item.icon}}" alt="{{item.title}}" />
                                        {% endif %}
                                    </div>
                                    <span class="t-boats__item-title">{{item.title}}</span>
                                </a>
                            </swiper-slide>
                        {% endfor %}
                    </swiper>
                </div>
            </div>
        </div>
    </header>
</header-cmp>
