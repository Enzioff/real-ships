<social-networks-cmp inline-template>
    <div class="t-social-networks">
        {% for item in social %}
            <a href="{{ item.link }}" target="_blank" rel="nofollow" class="t-social-networks__item t-a-btn-circle">
                <svg class="t-circle-svg" preserveAspectRatio="none" viewBox="0 0 338 172">
                    <path vector-effect="non-scaling-stroke" d="M169,3.5c91.1,0,165,36.9,165,82.5s-73.9,82.5-165,82.5S4,131.6,4,86C4,40.4,77.9,3.5,169,3.5"></path>
                </svg>
                <div style="background-image: url({{ item.icon }})"></div>
            </a>
        {% endfor %}
    </div>
</social-networks-cmp>
