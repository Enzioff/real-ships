</head>

<body>
<div id="app" :class="{'t-loaded': firstLoad}" >
    {% include "@components/base/header/template.twig" with menu %}
    <transition name="page">
        <keep-alive>
            <router-view :class="pageClass" class="t-page" >
                <div id="content" v-if="false">
                    <transition>
                        {% block content %} {% endblock %}
                    </transition>
                </div>
            </router-view>
        </keep-alive>
    </transition>
    {% include "@components/base/footer/template.twig" with menu %}
    {% include "@components/base/curtain/template.twig" %}
    <modal-cookie-cmp></modal-cookie-cmp>
</div>

<script>
    {# перемная содержащая все статические переводы для компонентов #}
    var staticText = {{ staticText|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var locale = {{ locale|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var social = {{ social|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var email = {{ email|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var phones = {{ phones|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var address = {{ address|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
    var footerPhone = {{ footerPhone|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }};
</script>

{% block body_end %} {% endblock %}
{{ body|raw }}
</body>
